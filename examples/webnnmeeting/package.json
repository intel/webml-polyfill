{
  "name": "webnnmeeting",
  "version": "1.0.0",
  "description": "A video conference example based on W3C Web Neural Network API and Open WebRTC Toolkit (OWT).",
  "author": "Zhang, Belem",
  "private": true,
  "scripts": {
    "dev": "concurrently \"nodemon server/meetingserver.js\" \"cross-env NODE_ENV=development nodemon server/index.js --watch server\"",
    "build": "nuxt build",
    "start": "concurrently \"nodemon server/meetingserver.js\" \"cross-env NODE_ENV=production node server/index.js\"",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.13.2",
    "nuxt-buefy": "^0.3.31",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@vue/test-utils": "^1.0.3",
    "ava": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^2.6.0",
    "lint-staged": "^13.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.16.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0"
  }
}
